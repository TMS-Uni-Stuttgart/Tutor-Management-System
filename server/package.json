{
    "name": "server",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist temp",
        "build": "pnpm prebuild && nest build --path \"./tsconfig.build.json\"",
        "format": "eslint --fix \"./**/*.ts?(x)\"",
        "lint": "pnpm run eslint:check",
        "eslint:check": "eslint \"./**/*.ts?(x)\"",
        "prettier:check": "prettier --check \"./**/*.ts?(x)\"",
        "prettier:format": "prettier --write --loglevel warn \"./**/*.ts?(x)\"",
        "ts:check": "tsc --project tsconfig.check.json --noEmit --incremental false",
        "ts:check:watch": "tsc --project tsconfig.check.json --noEmit --incremental false -w",
        "start": "env-cmd cross-env NODE_ENV=development nest start -p ./tsconfig.dev.json --debug --watch",
        "start:timezone": "cross-env TZ='Europe/Berlin' pnpm start",
        "start:nowatch": "env-cmd cross-env NODE_ENV=development nest start",
        "start:prod": "node dist/main",
        "test": "env-cmd -f ./.env.test cross-env NODE_ENV=test jest --colors --verbose=true",
        "test:watch": "pnpm test --watch",
        "test:cov": "pnpm test --coverage",
        "test:debug:win": "env-cmd -f ./.env.test cross-env NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register ../node_modules/jest/bin/jest.js --runInBand",
        "derp": "jest --init",
        "test:e2e": "jest --config ./test/jest-e2e.json"
    },
    "dependencies": {
        "@mikro-orm/core": "^5.3.1",
        "@mikro-orm/mysql": "^5.3.1",
        "@mikro-orm/nestjs": "^5.1.1",
        "@mikro-orm/reflection": "^5.3.1",
        "@nestjs/common": "^9.0.11",
        "@nestjs/core": "^9.0.11",
        "@nestjs/passport": "^9.0.0",
        "@nestjs/platform-express": "^9.0.11",
        "@nestjs/swagger": "^6.1.0",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.20.0",
        "class-validator": "^0.13.2",
        "excel4node": "^1.8.0",
        "express-session": "^1.17.3",
        "highlight.js": "^10.7.2",
        "jszip": "^3.10.1",
        "luxon": "^1.25.0",
        "nodemailer": "^6.7.8",
        "papaparse": "^5.3.2",
        "passport": "^0.6.0",
        "passport-local": "^1.0.0",
        "pug": "^3.0.2",
        "puppeteer": "5.5.0",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^7.5.6",
        "swagger-ui-express": "^4.5.0",
        "uuid": "^8.3.2",
        "yaml": "^1.10.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.1.1",
        "@nestjs/schematics": "^9.0.1",
        "@nestjs/testing": "^9.0.11",
        "@types/bcryptjs": "^2.4.2",
        "@types/body-parser": "^1.19.2",
        "@types/express": "^4.17.13",
        "@types/express-serve-static-core": "^4.17.30",
        "@types/express-session": "^1.17.5",
        "@types/jest": "^28.1.8",
        "@types/luxon": "^1.25.1",
        "@types/node": "^18.7.13",
        "@types/nodemailer": "^6.4.5",
        "@types/papaparse": "^5.3.4",
        "@types/passport": "^1.0.10",
        "@types/passport-local": "^1.0.34",
        "@types/pug": "^2.0.6",
        "@types/puppeteer": "5.4.2",
        "@types/supertest": "^2.0.12",
        "@types/uuid": "^8.3.4",
        "jest": "^29.0.0",
        "supertest": "^6.2.4",
        "ts-jest": "^28.0.8",
        "ts-loader": "^9.3.1",
        "tsconfig-paths": "^4.1.0"
    }
}
